{
	"name": "flarum-lang/urdu",
	"type": "flarum-extension",
	"description": "Urdu language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/urdu",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"ur-locale",
		"urdu-locale",
		"urdu"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/urdu/issues",
		"source": "https://github.com/flarum-lang/urdu"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Urdu",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ur",
			"title": "Urdu"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
